(executables (names JsPrettyPrinterApp)
    (modules (:standard \ JsPrettyPrinterBinPesyModules))
    (public_names JsPrettyPrinterApp)
    (libraries js-pretty-printer.bin.pesy-modules)
    (flags -opaque -open JsPrettyPrinterBinPesyModules))
(library (public_name js-pretty-printer.bin.pesy-modules)
    (name JsPrettyPrinterBinPesyModules)
    (modules JsPrettyPrinterBinPesyModules)
    (libraries js-pretty-printer.library))
(rule
    (with-stdout-to JsPrettyPrinterBinPesyModules.re
        (run echo "module Library = JsPrettyPrinterLibrary;")))
